{
  "name": "@mounasabet/database",
  "version": "0.1.0",
  "private": true,
  "main": "./index.ts",
  "types": "./index.ts",
  "scripts": {
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:seed": "prisma db seed",
    "db:seed:base": "tsx ./prisma/seed-base.ts",
    "db:seed:demo": "tsx ./prisma/seed-demo.ts",
    "db:seed:test": "tsx ./prisma/seed-test.ts",
    "db:seed:prod": "tsx scripts/production-seed.ts",
    "db:setup:prod": "psql $DATABASE_URL -f scripts/production-setup.sql",
    "db:reset": "prisma migrate reset --force",
    "db:studio": "prisma studio",
    "seed": "tsx scripts/seed-cli.ts",
    "seed:validate": "tsx scripts/validate-seeds.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@prisma/client": "5.8.0",
    "better-auth": "^1.2.12",
    "commander": "^12.1.0"
  },
  "devDependencies": {
    "@types/node": "^24.0.12",
    "@vitest/coverage-v8": "^1.0.0",
    "prisma": "5.8.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vitest": "^1.6.1"
  },
  "prisma": {
    "seed": "tsx ./prisma/seed.ts"
  }
}

{
  "name": "@mounasabet/unified-platform",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPatterns=src/components --testPathPatterns=src/lib",
    "test:integration": "jest --testPathPatterns=src/__tests__/integration",
    "test:api": "jest --testPathPatterns=src/app/api/__tests__",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:smoke": "node scripts/smoke-test.js",
    "test:load": "k6 run scripts/load-test.js",
    "test:e2e:headed": "playwright test --headed",
    "test:all": "npm run test && npm run test:e2e",
    "test:performance": "jest --testPathPatterns=performance",
    "test:real-data": "ts-node src/__tests__/run-real-data-tests.ts",
    "test:mock-detection": "jest src/__tests__/mock-data-detection.test.ts --verbose",
    "test:empty-database": "jest src/__tests__/integration/empty-database.test.tsx --verbose",
    "test:empty-states": "jest src/__tests__/empty-state-handling.test.tsx --verbose",
    "test:error-scenarios": "jest src/__tests__/error-scenarios.test.tsx --verbose",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:seed": "prisma db seed",
    "db:seed:base": "tsx ./prisma/seed-base.ts",
    "db:seed:demo": "tsx ./prisma/seed-demo.ts",
    "db:seed:test": "tsx ./prisma/seed-test.ts",
    "db:reset": "prisma migrate reset --force",
    "db:studio": "prisma studio",
    "type-check": "tsc --noEmit",
    "perf:audit": "lighthouse http://localhost:3000 --output=html --output-path=./performance-audit.html",
    "perf:bundle": "npm run build:analyze",
    "perf:test": "npm run test:performance"
  },

{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "lint": {},
    "dev": {
      "cache": false,
      "persistent": true,
      "env": ["AUTH_SECRET"]
    },
    "@weddni/ui#build": {
      "outputs": ["dist/**"]
    },
    "@weddni/client#build": {
      "dependsOn": ["@weddni/ui#build"],
      "outputs": ["dist/**"]
    }
  }
}
